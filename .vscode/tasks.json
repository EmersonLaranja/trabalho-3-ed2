{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Debugger",
      "command": "/usr/bin/gcc-9",
      "args": [
        "-g",
        "*.c",
        "-o",
        "${workspaceFolder}/trab3", "-lm"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}